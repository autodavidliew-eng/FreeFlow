{
  "name": "@freeflow/context-server",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "node -r ts-node/register/transpile-only -r tsconfig-paths/register src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@freeflow/config": "workspace:*",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
