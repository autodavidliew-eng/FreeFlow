{
  "name": "@freeflow/auth-contract",
  "version": "1.0.0",
  "description": "Shared authentication contract for FreeFlow application",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "authentication",
    "jwt",
    "keycloak",
    "oidc",
    "token",
    "types"
  ],
  "author": "FreeFlow Team",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@freeflow/config": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
